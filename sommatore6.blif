.model sommatore6
.inputs A6 A5 A4 A3 A2 A1 B1
.outputs O6 O5 O4 O3 O2 O1

# sommo bit per bit, portandomi ogni volta in avanti il resto

.subckt costante0 O=K
.subckt sommatore1 A=A1 B=B1 CIN=K O=O1 COUT=C1
.subckt sommatore1 A=A2 B=K CIN=C1 O=O2 COUT=C2
.subckt sommatore1 A=A3 B=K CIN=C2 O=O3 COUT=C3
.subckt sommatore1 A=A4 B=K CIN=C3 O=O4 COUT=C4
.subckt sommatore1 A=A5 B=K CIN=C4 O=O5 COUT=C5
.subckt sommatore1_noresto A=A6 B=K CIN=C5 O=O6

.search sommatore1.blif
.search sommatore1_noresto.blif
.search costante0.blif

.end
